{
    "IgnoredFiles": [
        ".licensing.json",
        ".git/**",
        ".gitignore",
        "**/*.pdf"
    ],
    "License": "gpl-3",
    "LicenseParameters": {
        "ProjectName": "foobar",
        "CopyrightYears": "19XX-20YY",
    },
    "CommentedFiles": {
        "*.py": {
            "CommentLineStart": "#  ",
            "InsertAtLine": 2
        },
        "*.cpp": {
            "BlockComments": {
                "BlockStart": "/* ",
                "BlockLine": " * ",
                "BlockEnd": " */"
            },
            "InsertAtLine": 1
        },
        "*.h": {
            "BlockComments": {
                "BlockStart": "/* ",
                "BlockLine": " * ",
                "BlockEnd": " */"
            },
            "InsertAtLine": 1
        },
        "*.txt": {
            "Uncommented": null,
            "InsertAtLine": 1
        }
    }
}

