{
    "IgnoredFiles": [
        ".git/",
        ".gitignore",
        "*.pdf"
    ],
    "License": "gpl-3",
    "LicenseParameters": {
        "ProjectName": "foobar",
        "CopyrightYears": "19XX-20YY",
    },
    "RequiresHeader": [
        {
            "Patterns": "*.py",
            "CommentSyntax": [ "#  " ],
            "InsertAtLine": 2
        },
        {
            "Patterns": [ "*.cpp", "*.h" ],
            "CommentSyntax": [ "/* ", " * ", " */" ],
            "InsertAtLine": 1
        },
        {
            "Patterns": "*.txt",
            "CommentSyntax": [ ],
            "InsertAtLine": 1
        }
    ]
}

